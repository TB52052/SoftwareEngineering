<!DOCTYPE html>
<html>
<head>
    <%- include('partials/global-head') %>
    <%- include('partials/main-page-head') %>
    <link rel="stylesheet" href="/styles/side-bar.css">
    <title>Study Planner - Dashboard</title>
</head>
<body>
    <div class="wrapper">
        <%- include('partials/side-bar') %>
        <div class="content">


            <h2>Select a Semester </h2>
            <select name="semester" id="seasonForm">
                  <% semesterInfo.forEach(function(semester) { %>
                   <option value="<%= semester.Season %>"><%= semester.Season %></option>
                  <% }); %>
                  <input type="submit">
            </select>


        </div>
    </div>
    <%- include('partials/error-box') %>
</body>
</html>
